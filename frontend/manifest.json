{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "My Apps"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css": ["scrollbars.css"],

      "js": ["jquery.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/filter-btn.svg"],
      "matches": ["https://mail.google.com/*"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
